{
    "name": "fritsion/cms",
    "description": "Fritsion CMS - Modern, lightweight and modular content management system.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Fritsion Contributor"
        }
    ],
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Fritsion\\": "app/",
            "Fritsion\\Modules\\": "modules/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}